{
	"name": "@dependency/serviceDynamicContent",
	"main": "./entrypoint/programmaticAPI",
	"version": "1.0.3",
	"scripts": {
		"test": "yarn run scriptManager @dependency/deploymentScript \".test.default({ testPath: ['./test'] })\"",
		"test-inspect": "export NODE_OPTIONS='--inspect-brk' && yarn run scriptManager @dependency/deploymentScript \".test.default({ testPath: ['./test'], shouldDebugger: true, shouldCompileTest: true })\"",
		"exportGraphData": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".graphDataLoading.exportAllGraphData({targetPath: './temporary/resource',fileName: 'data.graph.json'})\"",
		"loadGraphData": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".graphDataLoading.loadGraphDataFromFile({ shouldClearDatabase: true, graphDataFilePath: './resource/data.graph.json' })\"",
		"build": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".buildSourceCode.moduleProject({}, {compile:[/**'./test'*/]})\"",
		"buildAndRelease": "yarn run scriptManager shouldCompileScript=true @dependency/deploymentScript \".buildAndRelease.moduleProject({})\""
	},
	"flat": false,
	"private": true,
	"dependencies": {
		"@babel/core": "*",
		"@dependency/graphTraversal": "https://github.com/AppScriptIO/graphTraversal#semver:^4.x.x",
		"@dependency/graphTraversal-implementation": "https://github.com/AppScriptIO/graphTraversal-implementation#semver:^4.x.x",
		"@dependency/namedCurry": "https://github.com/AppScriptIO/namedCurry#semver:^1.x.x",
		"@dependency/streamToStringConvertion": "https://github.com/AppScriptIO/streamToStringConvertion#semver:^1.x.x",
		"@dependency/wrapStringStream": "https://github.com/AppScriptIO/wrapStringStream#semver:^1.x.x",
		"co-body": "*",
		"koa": "*",
		"koa-bodyparser": "^4.2.1",
		"koa-compress": "*",
		"koa-helmet": "*",
		"koa-json-error": "*",
		"koa-logger": "*",
		"koa-response-time": "*",
		"koa-sendfile": "*",
		"koa-sslify": "*",
		"koa-views": "*",
		"moment": "*",
		"multistream": "*",
		"openid-client": "*",
		"promise-events": "*",
		"ramda": "^0.26.1",
		"underscore": "*",
		"useragent": "*"
	},
	"devDependencies": {
		"@dependency/deploymentScript": "https://github.com/AppScriptIO/deploymentScript#semver:^2.x.x",
		"@dependency/javascriptStaticAnalysis": "https://github.com/AppScriptIO/javascriptStaticAnalysis#semver:^1.x.x",
		"@dependency/javascriptTranspilation": "https://github.com/AppScriptIO/javascriptTranspilation#semver:^1.x.x",
		"@dependency/scriptManager": "https://github.com/AppScriptIO/scriptManager#semver:^1.x.x",
		"typescript": "^3.3.3333",
		"user-agents": "^1.0.469"
	}
}
